<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff0f8ff"
    tools:context="com.example.nttr.quiz.ResultActivity"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="37dp"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="27dp"
        android:gravity="center_horizontal"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/backButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />


    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="116dp"

        android:text="リトライ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="238dp"
        android:layout_height="102dp"
        android:layout_marginBottom="14dp"
        android:layout_marginTop="80dp"
        android:gravity="center_horizontal"
        android:text="TextView"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginEnd="26dp"
        android:layout_marginStart="26dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView30"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView39"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView1"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView1"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toEndOf="@+id/imageView1"
            app:srcCompat="@drawable/heartoffline"
            tools:layout_editor_absoluteY="215dp" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
